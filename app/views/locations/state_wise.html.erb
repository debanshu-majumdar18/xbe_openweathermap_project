<div class="text-center">
  <h2>State-Wise Air Quality of <%= @states_aqi.count %> states </h2>
</div>
<div class="container" style="width: 700px">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Name</th>
          <th>Avg. AQI</th>
        </tr>
      </thead>
      <tbody>
     <% @states_aqi.each do |state| %>
        <tr>
          <td><%= state[0] %></td>
          <td><%= state[1] %></td>
        </tr>
      <% end %>
      </tbody>
    </table>
</div>